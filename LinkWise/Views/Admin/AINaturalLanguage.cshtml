@{
    ViewData["Title"] = "AI Natural Language";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div role="main" class="main" style="font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif">

    <div class="container container-xl-custom">
        <div class="row">
            <div class="col-lg-8 pt-5">
                <h2 class="font-weight-bold text-10 line-height-2 appear-animation" data-appear-animation="fadeInRightShorter" data-appear-animation-delay="500"><span class="text-5">Natural Language</span></h2>
                <h4 class="text-6 line-height-6 font-weight-normal appear-animation" data-appear-animation="fadeInRightShorter" data-appear-animation-delay="800"><span class="opacity-6">Soluções funcionais baseadas em inteligência artificial para o processamento e compreensão de linguagem natural.</span></h4>
            </div>
        </div>
    </div>
